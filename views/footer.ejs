<footer>
    <div class="footer-gridContainer">
        <div class="footer-logo">
            <a href="/"><img src="/images/logo.png" alt="logo"></a>
            <h1 class="footer-navTitle">Sarge's Invincible Consulting</h1>
            <link rel="stylesheet" href="/stylesheets/footer.css">
        </div>
        <ul class="footer-navBar">
            <% ['Home', 'Portfolio', 'Services', 'About', 'Contact'].forEach(element => { %>
                <li class="navItem <% if (element == currentPage) { %>active<% } %>">
                    <a href="/<% if (element == currentPage) { %>#<% } else { %><%= element %><% } %>">
                      <%= element %>
                    </a>
                  </li>
            <% }); %>
        </ul>
    </div>
    <p class="copyRight">&copy; 2024 Sarge's Invincible Consulting</p>
</footer>
